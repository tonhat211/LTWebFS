+			Unit u = unitList.get(this.getIndexUnitByProID(unitList, id));
+			Image i = imageList.get(this.getIndexImageByProID(imageList, id));
+			res.add(new ProductHeader(p, u, i));
+		}
+		
+		
+		return res;
